            <field name="name">Right </field>
            <field name="json_name">rig</field>
        </record>

        <record id="product_attribute_Oben_depending_on_entered_width" model="product.attribute">
            <field name="name">Top </field>
            <field name="json_name">top</field>
        </record>

        <record id="product_attribute_unten_depending_on_entered_width" model="product.attribute">
            <field name="name">Bottom </field>
            <field name="json_name">bot</field>
