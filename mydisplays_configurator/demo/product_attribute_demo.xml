<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_attribute_quantity" model="product.attribute">
            <field name="name">Quantity </field>
            <field name="val_custom" eval="True"/>
            <field name="custom_type">int</field>
        </record>

        <record id="product_attribute_systemvariante" model="product.attribute">
            <field name="name">System configuration </field>
            <field name="json_name">sc</field>
        </record>

        <record id="product_attribute_Größe" model="product.attribute">
            <field name="name">Size (width x height) </field>
            <field name="json_name">siz</field>
        </record>

        <record id="product_attribute_druckmaterial" model="product.attribute">
            <field name="name">Print material </field>
            <field name="json_name">pm</field>
        </record>

        <record id="product_attribute_MYD_easychange_system" model="product.attribute">
            <field name="name">MYD Easy Change System </field>
            <field name="json_name">mecs</field>
        </record>

        <record id="product_attribute_beleuchtung_mit_spot" model="product.attribute">
