<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="product_config_session_form_view" model="ir.ui.view">
        <field name="name">mydisplays.product.config.session.form</field>
        <field name="model">product.config.session</field>
        <field name="inherit_id" ref="product_configurator.product_config_session_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="json_vals" string="JSON Vals">
                    <field name="json_config_text" readonly="True" widget="ace" options="{'mode': 'python'}"/>
                    <field name="json_vals_debug" readonly="True" widget="ace" options="{'mode': 'python'}"/>
                </page>
            </xpath>
        </field>
  </record>

</odoo>
