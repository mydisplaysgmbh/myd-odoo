<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_template_form_view" model="ir.ui.view">
        <field name="name">mydisplays.product.configurator.product.template.form</field>
        <field name="model">product.template</field>
        <field name="priority">16</field>
        <field name="inherit_id" ref="product_configurator.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='config_step_line_ids']" position="after">
                <separator colspan="4" string="Computed Vals Formula"/>
                <field name="computed_vals_formula" widget="ace" options="{'mode': 'python'}"/>
<!--                 <separator colspan="4" string="Config Cache"/>
                <field name="config_cache"/> -->
            </xpath>
        </field>
    </record>

    <record id="product_form_view_custom_vals_remove" model="ir.ui.view">
        <field name="name">product.configurator.form.view.custom.vals.remove</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product_configurator.product_form_view_custom_vals_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook//field[@name='value_custom_ids']/.." position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
        </field>
    </record>

</odoo>